package practice;

import java.util.*;
//1:无需package
//2: 类名必须Main, 不可修改

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		String s=scan.next();
		char[]S=s.toCharArray();
		int l=0,r=S.length-1;
		while(l<r) {
			if(S[l]!=S[r]) {
				break;
				l+=1;
				r--;
			}
		}
		
		scan.close();
	}
}
