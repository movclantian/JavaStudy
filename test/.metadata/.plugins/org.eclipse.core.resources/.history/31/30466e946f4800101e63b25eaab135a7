import java.util.Scanner;

public class Main {
    private static final long p = 6421;
    private static long[] mi = new long[20240605];
    private static long[] tong = new long[6425];
    private static long ans;

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        for (int i = 1; i <= 20240601; ++i) {
            mi[i] = fastPow(i, i);
        }

        for (int i = 1; i <= 20240601; ++i) {
            ans += tong[(int) (p - mi[i]) % p];
            tong[(int) mi[i]]++;
        }

        System.out.println(ans);
    }

    private static long fastPow(long a, long b) {
        long sum = 1;
        a = a % p;
        while (b > 0) {
            if ((b & 1) == 1) {
                sum = (sum * a) % p;
            }
            a = (a * a) % p;
            b = b >> 1;
        }
        return sum % p;
    }
}