package practice;

import java.util.*;
//1:无需package
//2: 类名必须Main, 不可修改

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int[] a = new int[n];
		Set<Integer> set = new HashSet<>();
		for (int i = 0; i < a.length; i++) {
			a[i] = scan.nextInt();
			set.add(a[i]);
		}
		int m = set.size();
		int l = 1, r = n;
		while (l < r) {
			int mid = (l + r) / 2;
			if (check(mid, a, n)) {
				r = mid;
			} else {
				l = mid + 1;
			}
		}
		System.out.println(l);
		scan.close();
	}

	public static boolean check(int mid, int[] a, int m) {
		int n = a.length;
		int[] f = new int[1001];
		int l = 0, r = 0, ans = 0;
		while (r < n) {
			f[a[r]]++;
			if (f[a[r]]==l) {
				ans++;
			}
		}
		return false;

	}
}
